<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

<script>

    function AddAddition(){
        if($("#additionDetails").val() == 0)return false;
        $("#additionalTemp>ul").append('<div class="addelement"><li class="" data-id="'+  $("#additionDetails").val() +'"> <button  type="button" class="close pull-left" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>  '+ $( "#additionDetails option:selected" ).text() +' &nbsp;  &nbsp;  </li> </br></div>')
        $(".close").on("click",function () {
            $(this).closest('.addelement').remove();
        });
    }

    function AddDiscount(){
        if($("#selectDiscount").val() == 25)return false;
        $("#DiscountTemp>ul").append('<div class="addelement"><li class="" data-id="'+  $("#selectDiscount").val() +'"> <button  type="button" class="close pull-left" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>  '+ $( "#selectDiscount option:selected" ).text() +' &nbsp;  &nbsp;  </li> </br></div>')
        $(".close").on("click",function () {
            $(this).closest('.addelement').remove();
        });
    }

    $(document).ready(function () {


        var fees = 0;
        $.ajax({
            url: site['uri']['public'] + '/collectedfees/',
            type: "get",
            async: false,
        }).done(function (data) {
            data = JSON.parse(data);
            fees = data.value;
        }).fail(failureCallback);

        $.ajax({
            url: site['uri']['public'] + '/currencies/',
            type: "get",
        }).done(function (data) {
            data = JSON.parse(data);
            $('#currencies').empty();

            for (var i=0; i<data.length; i++) {
                $('#currencies').append('<option value="'+ data[i].symbol +'">'+ data[i].name +'</option>');
            }


        })


        var res = fees.split(",");
        for (var i = 0; i < res.length; i++) {
            $("#collectedFees_select").append('<option value="' + res[i] + '">' + res[i] + '</option>')
        }

        $("#country, #city").on("keyup", function () {
            $("#address").empty();
            $("#address").val($("#country").val() + "/ " + $("#city").val())
        })

        var flag = true;
        var totalprice = 0;
        $("#PaymentMethod_select").on("change",function () {
            flag = true;
            if(flag){
                flag = false;
                totalprice = $("#totalPrice").val();
            }
            var val = $(this).val();
            if(val == "Direct Installment"){
                $("#directInstallmentAdded").removeAttr('disabled');
            }else{
                $("#directInstallmentAdded").val("0");
                $("#totalPrice").val(parseInt(totalprice) + parseInt($("#directInstallmentAdded").val()));
                $("#directInstallmentAdded").attr('disabled','disabled')
            }
        })


        $("#directInstallmentAdded").on("keyup",function () {
            var directInstallmentAdded = parseInt($(this).val()) || 0;
            $("#totalPrice").val(parseInt(totalprice) + directInstallmentAdded);

        });

        var price_for_addition_discount_added = 0;
        var price_for_addition_discount_addedFlag = false;

        $("#discount_addition").on("keyup",function () {
            if(!price_for_addition_discount_addedFlag) {
                price_for_addition_discount_added = parseInt($("#totalPrice").val());
                price_for_addition_discount_addedFlag = true;
            }


            var discount_addition = parseInt($(this).val()) || 0;
            $("#totalPrice").val(price_for_addition_discount_added + discount_addition);
            totalprice = $("#totalPrice").val();
        })


    });
    var expanded = false;

    function showCheckboxes() {
        var checkboxes = document.getElementById("checkboxes");
        if (!expanded) {
            checkboxes.style.display = "block";
            expanded = true;
        } else {
            checkboxes.style.display = "none";
            expanded = false;
        }
    }


</script>
